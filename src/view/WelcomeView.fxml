<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<FlowPane orientation="VERTICAL" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.WelcomeController">
   <children>
      <HBox alignment="BOTTOM_LEFT" prefHeight="150.0" prefWidth="600.0">
         <children>
            <Label text="HELLO, ">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblUser" text="USER">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Label text="!">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="600.0">
         <children>
            <Label text="WELCOME BACK.">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" prefWidth="600.0">
         <children>
            <Label text="USER ID: ">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
            </Label>
            <Label fx:id="lblUserID" text="ID">
               <font>
                  <Font name="Montserrat Regular" size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</FlowPane>
